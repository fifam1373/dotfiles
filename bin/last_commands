#!/usr/bin/zsh
zsh -c 'history' | awk '{print $2}' | sort | uniq -c | sort -n | tail -20
exit
history |\
    awk '{print $2}' |\
    sed -E 's/^[[:space:]]*[0-9]+[[:space:]]+//' |\
    sed -E 's/^[[:space:]]*sudo[[:space:]]*//' |\
    sed -E 's/^([[:space:]]*(\-[^[:space:]]*)*([^[:space:]]+=("[^"]*"|[^[:space:]]*|\$?[(]+[^)]*[)]+)*)*[[:space:]]*)*//g'|\
    sed -E 's/^[[:space:]]*//' |\
    cut -d" " -f1 |\
    sort |\
    uniq -c |\ 
    sort -n --reverse |\
    head -n 20
